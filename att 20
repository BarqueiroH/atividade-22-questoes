import java.util.Scanner;

public class DuracaoJogo {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        System.out.print("Digite a hora de início (horas minutos): ");
        int horaInicio = scanner.nextInt();
        int minutoInicio = scanner.nextInt();

        System.out.print("Digite a hora de término (horas minutos): ");
        int horaFim = scanner.nextInt();
        int minutoFim = scanner.nextInt();

        int totalMinutosInicio = horaInicio * 60 + minutoInicio;
        int totalMinutosFim = horaFim * 60 + minutoFim;

        if (totalMinutosFim < totalMinutosInicio) {
            totalMinutosFim += 24 * 60;
        }

        int duracaoMinutos = totalMinutosFim - totalMinutosInicio;
        int duracaoHoras = duracaoMinutos / 60;
        int duracaoMinutosRestantes = duracaoMinutos % 60;

        System.out.printf("Duração do jogo: %d horas e %d minutos%n", duracaoHoras, duracaoMinutosRestantes);

        scanner.close();
    }
}
